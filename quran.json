[
    {
        "name": "الفاتحة",
        "verses": [
            {
                "arabic": "بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ",
                "translation": "بە نامی خوای مه‌ه‌ربانه‌ی مه‌رحمان"
            },
            {
                "arabic": "ٱلْحَمْدُ لِلَّهِ رَبِّ ٱلْعَٰلَمِينَ",
                "translation": "سەبارەیەکی تەماشاکردنی خۆیەتی پەیامەکان"
            },
            {
                "arabic": "ٱلرَّحْمَٰنِ ٱلرَّحِيمِ",
                "translation": "پەیامەکان خوێندندە مەحەربانیە"
            },
            {
                "arabic": "مَٰلِكِ يَوْمِ ٱلدِّينِ",
                "translation": "دەرەکی رووتەکان ڕووشەیەکان"
            },
            {
                "arabic": "إِيَّاكَ نَعْبُدُ وَإِيَّاكَ نَسْتَعِينُ",
                "translation": "پەیامەکان خۆیەتی جێگاکانی دەرکەی"
            },
            {
                "arabic": "ٱهْدِنَا ٱلصِّرَٰطَ ٱلْمُسْتَقِيمَ",
                "translation": "نووسراوی فەرمی زانیاری کە جەستەیەکی حکومەتی دیاری بەرزە"
            },
            {
                "arabic": "صِرَٰطَ ٱلَّذِينَ أَنْعَمْتَ عَلَيْهِمْ غَيْرِ ٱلْمَغْضُوبِ عَلَيْهِمْ وَلَا ٱلضَّآلِّينَ",
                "translation": "ئه‌وە ڕووناکەی درێژەیەتی"
            }
        ]
    },
    {
        "name": "البقرة",
        "verses": [
            {
                "arabic": "الم",
                "translation": "ئه‌وە ڕووناکەی درێژەیەتی"
            },
            {
                "arabic": "ذَٰلِكَ ٱلْكِتَٰبُ لَا رَيْبَ فِيهِ هُدًۭى لِّلْمُتَّقِينَ",
                "translation": "هەموو نووسراوی فەرمی زانیاری کە جەستەیەکی حکومەتی دیاری بەرزە"
            }
        ]
    }
]